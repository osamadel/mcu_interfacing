#include <avr/io.h>
#include <delay.h>

int main(void) {
	DDRA = 0b00000001;		// set PORTA.A0 to be OUTPUT
	PORTA = 0b00000000;		// set PORTA.A0 to LOW
	while (1) {
		PORTA = 0x01;
		_delay_ms(500);
		PORTA = 0x00;
		_delay_ms(500);
	}
	return 0;
}
