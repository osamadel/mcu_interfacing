#include "libraries/L_STD_TYPES.h"
//#include "HAL/HLCD_interface.h"
#include "MCAL/MDIO_interface.h"
#include <util/delay.h>

int main(void) {
//	HLCD_init();
	MDIO_SetPinDirection(PORTD, 0, OUTPUT);
	MDIO_SetPinDirection(PORTD, 1, OUTPUT);
	MDIO_SetPinDirection(PORTD, 2, OUTPUT);
	while(1) {
//		HLCD_writeData('O');
		MDIO_SetPinOutput(PORTD, 0, LOW);
		MDIO_SetPinOutput(PORTD, 1, LOW);
		MDIO_SetPinOutput(PORTD, 2, LOW);

		MDIO_SetPinOutput(PORTD, 0, HIGH);
		_delay_ms(1000);
		MDIO_SetPinOutput(PORTD, 1, HIGH);
		_delay_ms(1000);
		MDIO_SetPinOutput(PORTD, 2, HIGH);
		_delay_ms(1000);
	}// end while(1)
	return 0;
}
