#include "libraries/L_STD_TYPES.h"
#include "HAL/HLCD_interface.h"
#include <util/delay.h>


int main(void) {
	HLCD_init();
	while(1) {
		HLCD_clear();
		HLCD_setLine(1);
		writeText("Osama");
		_delay_ms(1000);
		writeText("Adel");
		_delay_ms(2000);
		HLCD_setLine(2);
		writeText("#Emb SW Eng#");
		_delay_ms(3000);
	}// end while(1)
	return 0;
}
